<%= form_for [@artist, @artist.songs.new] do |s| %>
  <div class="form-group">
    <%= s.label :track_number %>
    <br>
    <%= s.number_field :track_number %>
  </div>
  <div class="form-group">
    <%= s.label :name %>
    <br>
    <%= s.text_field :name %>
  </div>
  <div class="form-group">
    <%= s.label :duration %>
    <br>
    <%= s.text_field :duration %>
  </div>
  <div class="actions">
    <%= s.submit "Create" %>
  </div>
<% end %>
